<div class="container white">
  <%= form_for @admin, url: "/sessions", method: "post" do |f| %>
    <div>
      <%= f.text_field :email, placeholder: "Email" %>
    </div>
    <div>
      <%= f.password_field :password, placeholder: "Password" %>
    </div>
    <div class="col s6">
      <%= f.label(:avatar, "Upload an avatar", class:"blue-text") %>
      <br>
      <%= f.file_field :avatar %>
    </div>
    <%= f.submit "Sign In", class:"btn blue" %>
  <% end %>
</div>
